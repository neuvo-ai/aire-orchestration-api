{
	"name": "orchestration-api",
	"version": "1.1.0",
	"description": "An API.",
	"scripts": {
		"start": "npm run serve",
		"build": "npm run build-ts && npm run lint",
		"serve": "node dist/index.js",
		"watch-node": "nodemon dist/index.js",
		"watch": "IGNORE_SIGS=true concurrently -r -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"test": "jest --forceExit --coverage --verbose",
		"test-open-handles": "jest --coverage --verbose --detectOpenHandles",
		"watch-test": "npm run test -- --watchAll",
		"build-ts": "tsc",
		"build-ts-production": "tsc --sourceMap false",
		"watch-ts": "tsc -w",
		"lint": "eslint src",
		"lint-fix": "eslint src --fix",
		"debug": "npm run build && npm run watch-debug",
		"serve-debug": "nodemon --inspect dist/index.js",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
	},
	"author": "Neuvo Inc. Global",
	"license": "UNLICENSED",
	"devDependencies": {
		"@types/body-parser": "^1.17.0",
		"@types/errorhandler": "^1.5.0",
		"@types/express": "^4.17.8",
		"@types/express-jwt": "^6.0.1",
		"@types/is-base64": "^1.1.0",
		"@types/jest": "^26.0.15",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/lodash": "^4.14.164",
		"@types/morgan": "^1.9.2",
		"@types/node": "^15.6.1",
		"@types/response-time": "^2.3.3",
		"@types/supertest": "^2.0.10",
		"@typescript-eslint/eslint-plugin": "^4.6.1",
		"@typescript-eslint/parser": "^4.6.1",
		"chai": "^4.2.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.12.1",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.7.6",
		"eslint-plugin-prefer-arrow": "^1.2.2",
		"jest": "^26.6.2",
		"jsonwebtoken": "^8.5.1",
		"nodemon": "^2.0.6",
		"ts-jest": "^26.4.3",
		"typescript": "^4.0.5"
	},
	"dependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/sanitize-html": "^2.3.1",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"errorhandler": "^1.5.1",
		"eslint-plugin-no-null": "^1.0.2",
		"express": "^4.17.1",
		"express-jwt": "^6.0.0",
		"express-jwt-permissions": "^1.3.3",
		"express-validator": "^6.13.0",
		"generate-password": "^1.6.1",
		"is-base64": "^1.1.0",
		"is-valid-hostname": "^1.0.1",
		"lodash": "^4.17.21",
		"mongoose": "^6.0.11",
		"morgan": "^1.9.1",
		"response-time": "^2.3.2",
		"salt-api": "^1.1.3",
		"sanitize-html": "^2.5.2",
		"supertest": "^6.0.0",
		"winston": "^3.3.3",
		"zlib": "^1.0.5"
	}
}
